Tab4:AddButton({
	Name = "TP to Arena",
	Callback = function()
local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()

-- Create a part to serve as the safe spot
local safeSpot = Instance.new("Part")
safeSpot.Size = Vector3.new(50, 1, 50)
safeSpot.Position = character.HumanoidRootPart.Position + Vector3.new(10, -10, -250)
safeSpot.Anchored = true
safeSpot.CanCollide = true
safeSpot.Transparency = 1
safeSpot.BrickColor = BrickColor.new("Bright white")
safeSpot.Parent = workspace
            wait(1)
character.HumanoidRootPart.CFrame = safeSpot.CFrame
      		print("button pressed")
  	end    
})

